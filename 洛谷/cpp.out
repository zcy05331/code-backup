"struct mod_t",
"{",
"    int x;",
"    mod_t() {}",
"    mod_t(ll v) : x(v) {}",
"    mod_t(int v) : x(v) {}",
"    static int Mod(int x) { return ((x < 0) ? (x + mod) : x); }",
"    mod_t operator+(const mod_t y) const { return Mod(x + y.x - mod); }",
"    mod_t operator-(const mod_t y) const { return Mod(x - y.x); }",
"    mod_t operator*(const mod_t y) const { return (ll)x * y.x % mod; }",
"};",
